<div class="container">

	<% if user.cuisine_id %>
  <%= link_to user_path(user) do %>
		<% if user.photo.attached? %>
			<%= cl_image_tag user.photo.key, class: "avatar" %>
		<% else %>
			<%= image_tag "https://media.wired.com/photos/5a13e24f63f8d15055c806fe/master/pass/hulk-still-bg.jpg", class: "avatar" %>
      <% end %>
		<% end %>
	<% end %>


    <p><%= user.first_name %> <%= user.last_name[0] %>.</p>
</div>